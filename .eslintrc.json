{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier",
    "plugin:import/errors",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "no-console": "off",
    "no-undef": "off"
  }
}
